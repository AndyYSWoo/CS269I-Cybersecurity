    "summaries": [
        {
            "category": "team", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 4705, 
            "content": "A Department of Defense webserver was vulnerable to a SQL injection attack that could have revealed sensitive financial information. @psychomantis was able to demonstrate this vulnerability by crafting specially formatted URLs. Thank you!\n", 
            "content_html": "<p>A Department of Defense webserver was vulnerable to a SQL injection attack that could have revealed sensitive financial information. <a href=\"/psychomantis\">@psychomantis</a> was able to demonstrate this vulnerability by crafting specially formatted URLs. Thank you!</p>\n"
        }, 
        {
            "category": "researcher", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 3180, 
            "content": "The service being used didn't properly sanitize id parameter. This resulted in a Blind SQL Injection vulnerability and it was demonstrated by making the SQL server wait for a period of time before responding.", 
            "content_html": "<p>The service being used didn&#39;t properly sanitize id parameter. This resulted in a Blind SQL Injection vulnerability and it was demonstrated by making the SQL server wait for a period of time before responding.</p>\n"
        }
    ], 
