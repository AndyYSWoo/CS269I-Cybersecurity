                "url": "/smit", 
                "profile_picture_urls": {
                    "medium": "https://profile-photos.hackerone-user-content.com/000/035/881/45cf6f3a8e40c88d84720c62b73d3fa6f16a2147_medium.JPG?1539131983"
                }, 
                "hackerone_triager": false
