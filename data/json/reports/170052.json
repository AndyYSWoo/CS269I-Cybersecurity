    "is_member_of_team?": null, 
    "vote_count": 4, 
    "summaries": [
        {
            "category": "team", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 2948, 
            "content": "@skorov discovered a vulnerability that allows a user can gain access to S3. This Vulnerability has been fixed.", 
            "content_html": "<p><a href=\"/skorov\">@skorov</a> discovered a vulnerability that allows a user can gain access to S3. This Vulnerability has been fixed.</p>\n"
        }, 
        {
            "category": "researcher", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 2950, 
            "content": "An information disclosure vulnerability was found that allowed an attacker to have arbitrary AWS requests signed with the secret key. The signature could then be used to view, edit, upload and delete any file in the S3 bucket.\n\nDigitalSellz team had great communication and acted quickly to remediate the vulnerability.", 
            "content_html": "<p>An information disclosure vulnerability was found that allowed an attacker to have arbitrary AWS requests signed with the secret key. The signature could then be used to view, edit, upload and delete any file in the S3 bucket.</p>\n\n<p>DigitalSellz team had great communication and acted quickly to remediate the vulnerability.</p>\n"
        }
    ], 
