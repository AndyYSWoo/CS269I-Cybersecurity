            "is_internal": false, 
            "editable": false, 
            "updated_at": "2015-01-27T15:23:42.260Z", 
            "actor": {
                "username": "aarmageddon", 
                "url": "/aarmageddon", 
                "profile_picture_urls": {
                    "medium": "https://profile-photos.hackerone-user-content.com/000/012/940/6bfec905adec6f7b3520bc23e78adaf16e7707da_medium.jpg?1447324087"
                }, 
                "hackerone_triager": false
