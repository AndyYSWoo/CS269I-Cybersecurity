    "summaries": [
        {
            "category": "team", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 1773, 
            "content": "@iad found an SQL Injection vulnerability in one of our Wordpress blog's plugins (the website being `love.uber.com`). This blog was hosted at WPEngine and did not contain any of our user's information. However due to our previously vague bug bounty rules we decided to reward the maximum of 3,000$ since we were not previously clear that these were maximums. Opting for limited disclosure as password hashes are disclosed in the report which we need to ensure have been rotated properly.", 
            "content_html": "<p><a href=\"/iad\">@iad</a> found an SQL Injection vulnerability in one of our Wordpress blog&#39;s plugins (the website being <code>love.uber.com</code>). This blog was hosted at WPEngine and did not contain any of our user&#39;s information. However due to our previously vague bug bounty rules we decided to reward the maximum of 3,000$ since we were not previously clear that these were maximums. Opting for limited disclosure as password hashes are disclosed in the report which we need to ensure have been rotated properly.</p>\n"
        }, 
        {
            "category": "researcher", 
            "can_view?": true, 
            "can_edit?": false, 
            "id": 2110, 
            "content": "Vulnerability was found in WPML module.\nKudos to Jouko Pynn\u00f6nen, who found this bug in WPML module ( https://klikki.fi/adv/wpml.html )", 
            "content_html": "<p>Vulnerability was found in WPML module.<br>\nKudos to Jouko Pynn\u00f6nen, who found this bug in WPML module ( <a title=\"https://klikki.fi/adv/wpml.html\" href=\"/redirect?signature=d31547bbb9eaba17b5be11d84ce6162566638558&amp;url=https%3A%2F%2Fklikki.fi%2Fadv%2Fwpml.html\" target=\"_blank\" rel=\"nofollow noopener noreferrer\"><span>https://klikki.fi/adv/wpml.html</span><i class=\"icon-external-link\"></i></a> )</p>\n"
        }
    ], 
